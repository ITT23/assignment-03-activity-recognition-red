import argparse
 
def  get_parsed_arguments():
    parser = argparse.ArgumentParser()
 
    # data related arguments
    parser.add_argument(
        "-activity", help="Name of the activity you want to collect data for (one of: waving, standing, punching)."
    )
 
    parser.add_argument(
        "-number", help="Number of this csv file (0-9)"
    )
 
 
    return parser.parse_args()
